<?xml version='1.0' encoding='UTF-8'?>
<webservices xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <webservice-description>
    <webservice-description-name>com.alsb.hello.HelloWorld</webservice-description-name>
    <wsdl-file>WEB-INF/HelloWorldService.wsdl</wsdl-file>
    <jaxrpc-mapping-file>WEB-INF/HelloWorldService.xml</jaxrpc-mapping-file>
    <port-component>
      <port-component-name>HelloWorldSoapPort</port-component-name>
      <wsdl-port xmlns:alsb="http://www.alsb.com/">alsb:HelloWorldSoapPort</wsdl-port>
      <service-endpoint-interface>com.alsb.hello.HelloWorldPortType</service-endpoint-interface>
      <service-impl-bean>
        <servlet-link>HelloWorldServiceServlethttp</servlet-link>
      </service-impl-bean>
    </port-component>
  </webservice-description>
</webservices>
